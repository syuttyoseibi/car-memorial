{
  "version": 2,
  "builds": [
    { "src": "server.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/generate-story", "dest": "server.js" },
    { "src": "/download-pdf", "dest": "server.js" },
    { "src": "/style.css", "dest": "/style.css" },
    { "src": "/script.js", "dest": "/script.js" },
    { "src": "/", "dest": "/index.html" }
  ]
}